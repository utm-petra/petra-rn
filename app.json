{
  "expo": {
    "name": "Petra",
    "description": "Explore UTM's rock collection",
    "slug": "petra-rn",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.utm.petra",
      "supportsTablet": true
    },
    "android": {
      "package": "com.utm.petra",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#EDE4D9"
      }
    },
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EDE4D9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"]
  }
}
